[section#spanbuf.assign
    [:en] Assignment and swap
    [:zh_CN] 赋值和交换
]

[%@lib@member operator=[!basic_spanbuf]]
[codeblock:declaration]
basic_spanbuf& operator=(basic_spanbuf&& rhs);
[codeblock:end]

[div:description]
[para:effects]
[:en] Equivalent to:
[:zh_CN] 等价于：
[codeblock]
basic_spanbuf tmp{std::move(rhs)};
this->swap(tmp);
return *this;
[codeblock:end]
[div:end]

[%@lib@member swap[!basic_spanbuf]]
[codeblock:declaration]
void swap(basic_spanbuf& rhs);
[codeblock:end]

[div:description]
[para:effects]
[:en] Equivalent to:
[:zh_CN] 等价于：
[codeblock:notation]
basic_streambuf<charT, traits>::swap(rhs);
std::swap([*mode], rhs.[*mode]);
std::swap([*buf], rhs.[*buf]);
[codeblock:end]
[div:end]

[%@lib@member swap[!basic_spanbuf]]
[codeblock:declaration]
template<class charT, class traits>
  void swap(basic_spanbuf<charT, traits>& x, basic_spanbuf<charT, traits>& y);
[codeblock:end]

[div:description]
[para:effects]
[:en] Equivalent to: [`x.swap(y)].
[:zh_CN] 等价于：[`x.swap(y)]。
[div:end]
