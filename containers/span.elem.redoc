[section#span.elem
    [:en] Element access
    [:zh_CN] 成员访问
]

[%@library operator`[`][!span]]
[codeblock:declaration]
constexpr reference operator[](index_type idx) const;
[codeblock:end]

[enter:description]
[para:expects]
[:en] [`idx < size()] is [`true].
[:zh_CN] [`idx < size()] 为 [`true]。

[para:effects]
[:en] Equivalent to: [`return *(data() + idx);]
[:zh_CN] 等价于：[`return *(data() + idx);]
[exit:description]

[%@library span[!front]]
[%@library front[!span]]
[codeblock:declaration]
constexpr reference front() const;
[codeblock:end]

[enter:description]
[para:expects]
[:en] [`empty()] is [`false].
[:zh_CN] [`empty()] 为 [`false]。

[para:effects]
[:en] Equivalent to: [`return *data();]
[:zh_CN] 等价于：[`return *data();]
[exit:description]

[%@library span[!back]]
[%@library back[!span]]
[codeblock:declaration]
constexpr reference back() const;
[codeblock:end]

[enter:description]
[para:expects]
[:en] [`empty()] is [`false].
[:zh_CN] [`empty()] 为 [`false]。

[para:effects]
[:en] Equivalent to: [`return *(data() + (size() - 1));]
[:zh_CN] 等价于：[`return *(data() + (size() - 1));]
[exit:description]

