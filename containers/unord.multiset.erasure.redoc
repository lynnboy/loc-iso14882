[section#unord.multiset.erasure
    [:en] Erasure
    [:zh_CN] 擦除
]

[%@library erase_if[!unordered_multiset]]
[codeblock:declaration]
template<class K, class H, class P, class A, class Predicate>
  void erase_if(unordered_multiset<K, H, P, A>& c, Predicate pred);
[codeblock:end]

[begin:description]
[para:effects]
[:en] Equivalent to:
[:zh_CN] 等价于：
[codeblock:notation]
for (auto i = c.begin(), last = c.end(); i != last; ) {
  if (pred(*i)) {
    i = c.erase(i);
  } else {
    ++i;
  }
}
[codeblock:end]
[end:description]
