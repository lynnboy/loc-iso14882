[table:grid#support.contract.enum.semantic
    [:en] Enum [`evaluation_semantic]
    [:zh_CN] 枚举 [`evaluation_semantic]
    [|  [:en] Name
        [:zh_CN] 名称 ]
    [|  [:en] Meaning
        [:zh_CN] 含义 ]
]

[|] [`ignore]
[|] [:en] Ignore evaluation semantic
    [:zh_CN] 忽略评估语义
[-]
[|] [`observe]
[|] [:en] Observe evaluation semantic
    [:zh_CN] 观察评估语义
[-]
[|] [`enforce]
[|] [:en] Enforce evaluation semantic
    [:zh_CN] 强制评估语义
[-]
[|] [`quick_enforce]
[|] [:en] Quick-enforce evaluation semantic
    [:zh_CN] 快速强制评估语义
