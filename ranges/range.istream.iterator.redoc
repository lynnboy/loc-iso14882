[%@lib basic_istream_view::iterator]
[codeblock:synopsis]
namespace std::ranges {
  template<movable Val, class CharT, class Traits>
    requires default_initializable<Val> &&
             [[redoc("*:c>")]]stream-extractable<Val, CharT, Traits>
  class basic_istream_view<Val, CharT, Traits>::[[redoc("*>")]]iterator {      // [=expos]
  public:
    using iterator_concept = input_iterator_tag;
    using difference_type = ptrdiff_t;
    using value_type = Val;

    [[redoc("*>")]]iterator() = default;
    constexpr explicit [[redoc("*>")]]iterator(basic_istream_view& parent) noexcept;

    [[redoc("*>")]]iterator(const [[redoc("*>")]]iterator&) = delete;
    [[redoc("*>")]]iterator([[redoc("*>")]]iterator&&) = default;

    [[redoc("*>")]]iterator& operator=(const [[redoc("*>")]]iterator&) = delete;
    [[redoc("*>")]]iterator& operator=([[redoc("*>")]]iterator&&) = default;

    [[redoc("*>")]]iterator& operator++();
    void operator++(int);

    Val& operator*() const;

    friend bool operator==(const [[redoc("*>")]]iterator& x, default_sentinel_t);

  private:
    basic_istream_view* [[redoc("*>")]]parent_ = nullptr;                      // [=expos]
  };
}
[codeblock:end]

[%@lib@ctor basic_istream_view::iterator]
[codeblock:declaration]
constexpr explicit [[redoc("*>")]]iterator(basic_istream_view& parent) noexcept;
[codeblock:end]

[div:description]
[para:effects]
[:en] Initializes [*parent_] with [`addressof(parent)].
[:zh_CN] 以 [`addressof(parent)] 初始化 [*parent_]。
[div:end]

[%@lib@member operator++[!basic_istream_view::iterator]]
[codeblock:declaration]
[[redoc("*>")]]iterator& operator++();
[codeblock:end]

[div:description]
[para:expects]
[:en] [`[*parent_]->[*stream_] != nullptr] is [`true].
[:zh_CN] [`[*parent_]->[*stream_] != nullptr] 为 [`true]。

[para:effects]
[:en] Equivalent to:
[:zh_CN] 等价于：
[codeblock]
*[[redoc("*>")]]parent_->[[redoc("*>")]]stream_ >> [[redoc("*>")]]parent_->[[redoc("*>")]]object_;
return *this;
[codeblock:end]
[div:end]

[%@lib@member operator++[!basic_istream_view::iterator]]
[codeblock:declaration]
void operator++(int);
[codeblock:end]

[div:description]
[para:expects]
[:en] [`[*parent_]->[*stream_] != nullptr] is [`true].
[:zh_CN] [`[*parent_]->[*stream_] != nullptr] 为 [`true]。

[para:effects]
[:en] Equivalent to [`++*this].
[:zh_CN] 等价于 [`++*this]。
[div:end]

[%@lib@member operator*[!basic_istream_view::iterator]]
[codeblock:declaration]
Val& operator*() const;
[codeblock:end]

[div:description]
[para:expects]
[:en] [`[*parent_]->[*stream_] != nullptr] is [`true].
[:zh_CN] [`[*parent_]->[*stream_] != nullptr] 为 [`true]。

[para:effects]
[:en] Equivalent to: [`return [*parent_]->[*object_];]
[:zh_CN] 等价于：[`return [*parent_]->[*object_];]
[div:end]

[%@lib@member operator==[!basic_istream_view::iterator]]
[codeblock:declaration]
friend bool operator==(const [[redoc("*>")]]iterator& x, default_sentinel_t);
[codeblock:end]

[div:description]
[para:effects]
[:en] Equivalent to: [`return x.[*parent_] == nullptr || !*x.[*parent_]->[*stream_];]
[:zh_CN] 等价于：[`return x.[*parent_] == nullptr || !*x.[*parent_]->[*stream_];]
[div:end]
