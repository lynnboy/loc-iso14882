[%@lib@member operator-[!iota_view::iterator]]
[codeblock:declaration]
friend constexpr difference_type operator-(const iterator& x, const iterator& y)
  requires [[redoc("*:c", "n>")]]advanceable<W>;
[codeblock:end]

[div:description]
[para:effects]
[:en] Equivalent to:
[:zh_CN] 等价于：
[codeblock]
using D = difference_type;
if constexpr ([[redoc("^", "n>")]]is-integer-like<W>) {
  if constexpr ([[redoc("^", "n>")]]is-signed-integer-like<W>)
    return D(D(x.value_) - D(y.value_));
  else
    return (y.value_ > x.value_)
      ? D(-D(y.value_ - x.value_))
      : D(x.value_ - y.value_);
} else {
  return x.value_ - y.value_;
}
[codeblock:end]
