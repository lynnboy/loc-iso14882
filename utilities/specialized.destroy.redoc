[section#specialized.destroy
    [`destroy]
]

[%@library destroy_at]
[codeblock:declaration]
template<class T>
  void destroy_at(T* location);
[codeblock:end]

[enter:description]
[para:effects]
[:en] Equivalent to:
[:zh_CN] 等价于：

[codeblock:notation]
location->~T();
[codeblock:end]
[exit:description]

[%@library destroy]
[codeblock:declaration]
template<class ForwardIterator>
  void destroy(ForwardIterator first, ForwardIterator last);
[codeblock:end]

[enter:description]
[para:effects]
[:en] Equivalent to:
[:zh_CN] 等价于：

[codeblock:notation]
for (; first!=last; ++first)
  destroy_at(addressof(*first));
[codeblock:end]
[exit:description]

[%@library destroy_n]
[codeblock:declaration]
template<class ForwardIterator, class Size>
  ForwardIterator destroy_n(ForwardIterator first, Size n);
[codeblock:end]

[enter:description]
[para:effects]
[:en] Equivalent to:
[:zh_CN] 等价于：

[codeblock:notation]
for (; n > 0; (void)++first, --n)
  destroy_at(addressof(*first));
return first;
[codeblock:end]
[exit:description]
