[section#arithmetic.operations.divides
    [:en] Class template [`divides]
    [:zh_CN] 类模板 [`divides]
]

[%@library divides]
[codeblock:declaration]
template<class T = void> struct divides {
  constexpr T operator()(const T& x, const T& y) const;
};
[codeblock:end]

[%@library operator()[!divides]]
[%@library divides[!operator()]]
[codeblock:declaration]
constexpr T operator()(const T& x, const T& y) const;
[codeblock:end]

[div:description]
[para:returns]
[:en] [`x / y].
[:zh_CN] [`x / y]。
[div:end]

[%@library divides<>]
[codeblock:declaration]
template<> struct divides<void> {
  template<class T, class U> constexpr auto operator()(T&& t, U&& u) const
    -> decltype(std::forward<T>(t) / std::forward<U>(u));

  using is_transparent = [[redoc("=unspec")]];
};
[codeblock:end]

[%@library operator()[!divides<>]]
[%@library divides<>[!operator()]]
[codeblock:declaration]
template<class T, class U> constexpr auto operator()(T&& t, U&& u) const
    -> decltype(std::forward<T>(t) / std::forward<U>(u));
[codeblock:end]

[div:description]
[para:returns]
[:en] [`std::forward<T>(t) / std::forward<U>(u)].
[:zh_CN] [`std::forward<T>(t) / std::forward<U>(u)]。
[div:end]
